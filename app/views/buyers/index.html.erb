    <h1><%= current_user.first_name %>&nbsp;<%= current_user.last_name %></h1>
    	<table class="table", style="text-align: center;">
    		<% @books.each do |book| %>
          <tr>
            <td><%= book.book_name %></td>
            <td><%= book.author_name %></td>
            <td><%= book.price %></td>
            <td><%= link_to 'More Details', book_path(book) %></td>
          </tr>
        <% end %>
      </table>